{
  "name": "lmbdrp",
  "version": "0.0.0",
  "description": "Find artists and stay informed",
  "main": "app.js",
  "private": true,
  "scripts": {
    "start": "node ./dist/server.js",
    "start:watch": "nodemon -e js,html,css ./bin/www",
    "start:hotreload": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline"
    "build": "babel src -d dist",
    "babel": "babel src -d dist",
    "babel:watch": "babel -w src -d dist --source-maps",
    "webpack": "./node_modules/.bin/webpack -d",
    "webpack:watch": "./node_modules/.bin/webpack -d --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesse-weaver/lbmdrp.git"
  },
  "author": "jesse-weaver",
  "bugs": {
    "url": "https://github.com/jesse-weaver/lbmdrp/issues"
  },
  "homepage": "https://github.com/jesse-weaver/lbmdrp#readme",
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-favicon": "^1.0.1",
    "morgan": "~1.6.1",
    "nunjucks": "^2.1.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.5.0",
    "file-loader": "^0.9.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
