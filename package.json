{
  "name": "lmbdrp",
  "version": "0.0.0",
  "description": "Find artists and stay informed",
  "main": "app.js",
  "private": true,
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "eslint . --cache",
    "start": "node ./dist/server.js",
    "start:watch": "DEBUG=lbmdrop* nodemon -e js,html,css ./dist/server.js",
    "start:dev": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env development\" ./dist/server.js",
    "start:hotreload": "",
    "build": "babel src -d dist --source-maps && webpack --env production",
    "babel": "babel src -d dist --source-maps",
    "babel:watch": "babel -w src -d dist --source-maps",
    "webpack": "webpack --env production",
    "webpack:watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesse-weaver/lbmdrp.git"
  },
  "author": "jesse-weaver",
