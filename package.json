{
  "name": "lmbdrp",
  "version": "0.0.0",
  "description": "Find artists and stay informed",
  "main": "src/app/index.js",
  "private": true,
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "lint": "eslint . --cache",
    "start": "node ./dist/server.js",
    "start:watch": "DEBUG=lbmdrop* nodemon -e js,html,css ./dist/server.js",
    "start:dev": "npm run babel && nodemon --watch webpack.dev.js --exec \"cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js\"",
    "start:dev:webpack": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production babel src -d dist --source-maps && webpack --config config/webpack.prod.js",
    "babel": "babel src -d dist --source-maps",
    "babel:watch": "babel -w src -d dist --source-maps",
    "prettify": "prettier --write 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesse-weaver/lbmdrp.git"
  },
  "author": "jesse-weaver",
  "bugs": {
    "url": "https://github.com/jesse-weaver/lbmdrp/issues"
  },
  "homepage": "https://github.com/jesse-weaver/lbmdrp#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.3.5",
    "debug": "^2.6.9",
    "express": "4.17",
    "express-favicon": "^1.0.1",
    "fetch": "^1.1.0",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "node-cache": "^4.1.1",
    "node-fetch": "^2.5.0",
    "prop-types": "^15.7.2",
    "purecss": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "autoprefixer": "^10.4.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.0.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.5.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.19",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-middleware": "^5.3.3",
    "webpack-dev-server": "^4.10.1",
    "webpack-hot-middleware": "^2.25.2",
    "webpack-merge": "^5.8.0"
  }
}
